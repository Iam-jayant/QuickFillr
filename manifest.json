{
  "manifest_version": 3,
  "name": "Form Autofiller",
  "version": "1.0",
  "description": "Autofill forms with saved or fake info. Toggle dark mode. Manage info in settings.",
  "permissions": ["storage", "scripting"],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
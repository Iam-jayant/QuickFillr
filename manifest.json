
  
{
  "manifest_version": 3,
  "name": "QuickFillr",
  "version": "1.0",
  "description": "Autofill forms quickly with stored or extracted resume data.",
  "permissions": ["storage", "scripting"],
  "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
          "128": "Icons/root.png"
      }
  },
  "icons": {
      "128": "Icons/root.png"
  },
  "background": {
      "service_worker": "Scripts/background.js"
  },
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["Scripts/content.js"]
  }]
}